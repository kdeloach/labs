package  
{
	public class Margin 
	{
		public var top:Number;
		public var right:Number;
		public var bottom:Number;
		public var left:Number;
		
		public function Margin(top:Number, right:Number, bottom:Number, left:Number) 
		{
			this.top = top;
			this.right = right;
			this.bottom = bottom;
			this.left = left;
		}
	}
}